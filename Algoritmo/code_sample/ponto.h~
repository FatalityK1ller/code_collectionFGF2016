typedef struct ponto{
	float x;
	float y;
	float x1;
	float y1;
	float x2;
	float y2;

}Ponto;

void criaponto(Ponto *p,float x,float y);
void vervalores(Ponto *a);
void doispontos(Ponto *p, Ponto *c,float x1,float y1,float x2,float y2);
void vervalores2(Ponto *a, Ponto *b);
void quadrante(Ponto *a, Ponto *b,float x1,float y1,float x2,float y2);
void trocaX(Ponto *a, float x);
void trocaY(Ponto *a, float y);
void soma(Ponto *a,Ponto *b,float valorX1, float valorY1, float valorX2, float valorY2);
void versoma(Ponto *a, Ponto *b);
Ponto * exclui(Ponto *a);
