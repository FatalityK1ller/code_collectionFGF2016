algoritmo "semnome"
// Função :
// Autor :
// Data : 26/02/2016
// Seção de Declarações
var
numero:inteiro
inicio
// Seção de Comandos
escreval("digite um valor ")
leia(numero)

se (numero >=100) E (numero <=200) entao
  escreva("Valor esta entre 100 e 200")
senao
  escreva("valor não está entre 100 e 200")
fimse



fimalgoritmo
