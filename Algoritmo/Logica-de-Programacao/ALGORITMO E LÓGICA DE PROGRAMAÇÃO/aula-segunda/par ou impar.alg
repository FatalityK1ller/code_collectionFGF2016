algoritmo "semnome"
// Função :
// Autor :
// Data : 29/02/2016
// Seção de Declarações
var
numero:inteiro
inicio
// Seção de Comandos

escreva("informe um numero ")
leia(numero)

se numero mod 2=0 entao
   escreval("este numero é par ")
senao
   escreval("este numero é impar ")
fimse

se numero > 100 entao
   escreval("maior que 100")
senao
   escreval("menor que 100 ")
fimse
fimalgoritmo
