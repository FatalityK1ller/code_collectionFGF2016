algoritmo "semnome"
// Função :
// Autor :
// Data : 20/03/2016
// Seção de Declarações
var
n:real
n1,n2,n3:caractere
inicio
// Seção de Comandos

escreval("Digite o valor de N")
leia(n)

se n <= 10 entao
   n1<-("F1")
   escreval(n1)
fimse
se (n > 10) E (n <= 100) entao
   n2<-("F2")
   escreval(n2)
fimse
se n > 100 entao
   n3<-("F3")
   escreval(n3)
fimse


fimalgoritmo
