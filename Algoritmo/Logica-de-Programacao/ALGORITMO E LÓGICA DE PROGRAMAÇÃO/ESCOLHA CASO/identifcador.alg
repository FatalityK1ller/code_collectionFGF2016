algoritmo "identificador"
// Função :
// Autor :
// Data : 18/03/2016
// Seção de Declarações 
var
valor:inteiro
inicio
// Seção de Comandos

   escreval("Entre com o valor a ser testado")
   leia(valor)
   escolha valor
   caso 1,5,10
        escreva("O valor digitado é valido!!!")
   outrocaso
            escreva("Valor Inválido!!!")
   fimescolha

fimalgoritmo
